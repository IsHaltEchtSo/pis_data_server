{% extends 'components/base.jinja2' %}

{% block content %}
    {% for routine in data.routines %}
        {{ routine.starttime}}
        {{ routine.name }}
        <br>                            {% endfor %} 
        
    {% for zettel in data.zettels|sort(attribute = 'luhmann_id') %}
      	<a tag = "zettel_link" 
           href = " {{ url_for( 'zettelkasten_blueprint.view',   
                            	 luhmann_id = zettel.luhmann_id) }}    "  >
        	{{ zettel.luhmann_id }}: {{ zettel.title }}						</a>
        <br>                                                                        {% endfor %}
        
{% endblock content %}

        